[user]
    jaberg = James Bergstra
    name = James Bergstra
    email = james.bergstra@gmail.com
[alias]
    ci = commit
    cim = commit -m
    cimna = commit -m \"na\"
    co = checkout
    datetag = !git tag `date \"+%Y%m%d%H%M\"`
    st = "status -s"
    br = branch
    bra = "branch -a"
    fo = "fetch origin"
    fu = "fetch upstream"
    lg = "log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)[%an]%Creset' --abbrev-commit --date=relative"
    lgol = "!git --no-pager log --pretty=oneline"
[github]
    user = jaberg
[push]
	default = simple
